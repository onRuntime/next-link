{
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "next": ">=9",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@onruntime/next-link",
  "description": "Customized next/link for @onRuntime Next.js projects.",
  "author": "Antoine Kingue",
  "module": "dist/next-link.esm.js",
  "size-limit": [
    {
      "path": "dist/next-link.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-link.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "next": "^12.2.5",
    "styled-components": "^5.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/onRuntime/next-link.git",
  "bugs": {
    "url": "https://github.com/onRuntime/next-link/issues"
  },
  "homepage": "https://next-link-with-typescript.vercel.app/",
  "keywords": [
    "next",
    "nextjs",
    "link",
    "typescript",
    "styled-components"
  ]
}
